{
  "name": "project-engine",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run start:server & npm run start:react",
    "start:server": "nodemon server.js --exec babel-node --ignore './components' --ignore './public' -e js,jsx,graphql",
    "start:react": "nodemon --exec \"webpack\" --ignore './public' -e js,jsx",
    "build": "babel server.js -d dist",
    "release": "NODE_ENV=production node dist/server.js",
    "test": "NODE_ENV=test mocha ./__test__/setup.js **/__test__/**/*-test.js --colors --reporter min --compilers js:babel-register --require babel-polyfill"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "colors": "^1.1.2",
    "continuation-local-storage": "^3.2.0",
    "graphql": "^0.7.0",
    "handlebars": "^4.0.5",
    "hbs": "^4.0.1",
    "ini": "^1.3.4",
    "joi": "^9.0.4",
    "koa": "^2.0.0-alpha.7",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-graphql": "^0.5.6",
    "koa-mount": "^1.3.0",
    "koa-passport": "^2.2.2",
    "koa-router": "^7.0.1",
    "koa-session": "^3.3.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.1.2",
    "mysql": "^2.11.1",
    "redux-observable": "^0.12.0",
    "rxjs": "^5.0.0-beta.12",
    "sequelize": "^3.24.3",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-spies": "^0.7.1",
    "css-loader": "^0.25.0",
    "es6-promise": "^3.3.1",
    "graphql": "^0.7.0",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.2",
    "normalize.css": "^4.2.0",
    "react": "^15.3.2",
    "react-css-modules": "^3.7.10",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "sqlite3": "^3.1.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "whatwg-fetch": "^1.0.0"
  }
}
