{
  "name": "project-engine",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_PATH=./ nodemon server.js --exec babel-node",
    "build": "NODE_PATH=./ babel server.js -d dist",
    "release": "NODE_ENV=production NODE_PATH=./ node dist/server.js",
    "test": "NODE_ENV=test NODE_PATH=./ mocha test/setup.js test/**/*_test.js --colors --reporter min --compilers js:babel-register"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "colors": "^1.1.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "express-graphql": "^0.5.3",
    "graphql": "^0.6.2",
    "handlebars": "^4.0.5",
    "hbs": "^4.0.0",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "nodemon": "^1.10.2"
  }
}
